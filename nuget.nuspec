<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>datatables.net-scroller</id>
    <version>2.0.2</version>
    <description>Scroller for DataTables</description>
    <authors>SpryMedia Ltd</authors>
    <projectUrl>http://datatables.net</projectUrl>
    <license type="expression">MIT</license>
    <tags>scroller DataTables jQuery table </tags>
    <repository type="git" url="https://github.com/DataTables/Dist-DataTables-Scroller.git"/>
    <dependencies>
      <dependency id="jquery" version="1.7.0"/>
      <dependency id="datatables.net" version="1.10.15"/>
    </dependencies>
    <contentFiles>
      <files include="**/*" buildAction="Content"/>
    </contentFiles>
  </metadata>
  <files>
    <file src="js/datatables.scroller.js" target="content\Scripts"/>
    <file src="js/datatables.scroller.min.js" target="content\Scripts"/>
    <file src="types.d.ts" target="content\Scripts"/>
    <file src="js/datatables.scroller.js" target="contentFiles\any\any\wwwroot\js"/>
    <file src="js/datatables.scroller.min.js" target="contentFiles\any\any\wwwroot\js"/>
    <file src="types.d.ts" target="contentFiles\any\any\wwwroot\js"/>
  </files>
</package>
